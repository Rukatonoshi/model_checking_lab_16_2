// Вероятность активации сигнализации при возникновении пожара
P=? [ F(fire=1 & alarm_active=1) ] 

// Вероятность тушения пожара в течение 20 шагов после активации системы
P=? [ (suppression_active=1 & fire=1) U<=20 fire=0 ] 

// Вероятность ложного срабатывания
P=? [ (fire=0) U (alarm_active=1) ] 

